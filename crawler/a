<script  type="text/javascript">
    $(function () {
        try{
            var lib = eval("(" + $("#libkeywordjson").html() + ")");
            var html = "";
            if (lib.status) {
                $.each(lib.bases, function (i) {
                    var obj = lib.bases[i];  
                    html += '<dl class="expert_list">';
                    html += '      <dt><a href="' + obj.url + '" target="_blank"><img src="' + obj.logo + '" alt=""></a></dt>';
                    html += '      <dd>';
                    html += '          <a href="' + obj.url + '" class="recommend_t" target="_blank">' + obj.name.replace("知识库", "") + '</a>';
                    html += '         <div class="recomend_c"> ';              
                    html += '           <label>收录数：<span>' + obj.conCount + '</span></label>   <label>关注数：<span>' + obj.subCount + '</span></label> ';
                    html += '          </div>';
                    html += '      </dd>';
                    html += '</dl>';
                });
                if (html != "") {
                    setTimeout(function () {
                        $(".expert_wrap.recommend_lib").append(html);
                    }, 100);
                }
            }
        } catch (ex) { }
    });
</script>


        
